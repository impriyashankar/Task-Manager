<h1>My Tasks</h1>
<ul>
  <% @tasks.each do |task|%>
    <li> <%= link_to task.title, task_path(task.id)  %> <%= link_to "Delete #{task.title}",
            delete_path(task.id),
            method: :delete,
            data: { confirm: "Are you sure?" } %>
    </li>
  <% end %>
</ul>

<%= link_to "Add new task", new_path  %>
